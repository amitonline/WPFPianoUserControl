        <Rectangle x:Name="A" Grid.Row="0"  RadiusX="20" RadiusY="20" Grid.Column="5" Fill="Ivory" Height="Auto" Margin="0 0 0 0" StrokeThickness="2" Stroke="Black" MouseLeftButtonDown="A_MouseLeftButtonDown" MouseLeftButtonUp="A_MouseLeftButtonUp" MouseLeave="A_MouseLeave" MouseEnter="A_MouseEnter"></Rectangle>
        <Rectangle x:Name="GSharp2" Grid.Row="0" Grid.Column="5" Fill="Black" Height="300" VerticalAlignment="Top"  Width="40" HorizontalAlignment="Left" Margin="0 0 0 0 " StrokeThickness="1" Stroke="Black" MouseLeftButtonDown="GSharp2_MouseLeftButtonDown" MouseLeftButtonUp="GSharp2_MouseLeftButtonUp" MouseEnter="GSharp2_MouseEnter" MouseLeave="GSharp2_MouseLeave"></Rectangle>
