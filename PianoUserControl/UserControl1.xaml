<UserControl x:Class="PianoUserControl.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PianoUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" SizeChanged="UserControl_SizeChanged">
    <Grid SizeChanged="Grid_SizeChanged">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Rectangle x:Name="C" Grid.Row="0" RadiusX="20" RadiusY="20"  Grid.Column="0" Fill="Ivory" Height="Auto" Margin="0 0 0 0 " StrokeThickness="1" Stroke="Black" MouseLeftButtonDown="C_MouseLeftButtonDown" MouseLeftButtonUp="C_MouseLeftButtonUp" MouseLeave="C_MouseLeave" MouseEnter="C_MouseEnter"></Rectangle>
        <Rectangle x:Name="CSharp1" Grid.Row="0" Grid.Column="0" Fill="Black" Height="300" VerticalAlignment="Top"  Width="40" HorizontalAlignment="Right" Margin="0 0 0 0 " StrokeThickness="1" Stroke="Black" MouseLeftButtonDown="CSharp1_MouseLeftButtonDown" MouseLeftButtonUp="CSharp1_MouseLeftButtonUp" MouseEnter="CSharp1_MouseEnter" MouseLeave="CSharp1_MouseLeave"></Rectangle>
        <Rectangle x:Name="D" Grid.Row="0"  RadiusX="20" RadiusY="20" Grid.Column="1" Fill="Ivory" Height="Auto" Margin="0 0 0 0" StrokeThickness="2" Stroke="Black" MouseLeftButtonDown="D_MouseLeftButtonDown" MouseLeftButtonUp="D_MouseLeftButtonUp" MouseLeave="D_MouseLeave" MouseEnter="D_MouseEnter"></Rectangle>
        <Rectangle x:Name="CSharp2" Grid.Row="0" Grid.Column="1" Fill="Black" Height="300" VerticalAlignment="Top"  Width="40" HorizontalAlignment="Left" Margin="0 0 0 0 " StrokeThickness="1" Stroke="Black" MouseLeftButtonDown="CSharp2_MouseLeftButtonDown" MouseLeftButtonUp="CSharp2_MouseLeftButtonUp" MouseEnter="CSharp2_MouseEnter" MouseLeave="CSharp2_MouseLeave"></Rectangle>
        <Rectangle x:Name="DSharp1" Grid.Row="0" Grid.Column="1" Fill="Black" Height="300" VerticalAlignment="Top"  Width="40" HorizontalAlignment="Right" Margin="0 0 0 0 " StrokeThickness="1" Stroke="Black" MouseLeftButtonDown="DSharp1_MouseLeftButtonDown" MouseLeftButtonUp="DSharp1_MouseLeftButtonUp" MouseEnter="DSharp1_MouseEnter" MouseLeave="DSharp1_MouseLeave"></Rectangle>
