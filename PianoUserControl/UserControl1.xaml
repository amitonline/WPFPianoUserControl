        <Rectangle x:Name="DSharp2" Grid.Row="0" Grid.Column="2" Fill="Black" Height="300" VerticalAlignment="Top"  Width="40" HorizontalAlignment="Left" Margin="0 0 0 0 " StrokeThickness="1" Stroke="Black" MouseLeftButtonDown="DSharp2_MouseLeftButtonDown" MouseLeftButtonUp="DSharp2_MouseLeftButtonUp" MouseEnter="DSharp2_MouseEnter" MouseLeave="DSharp2_MouseLeave"></Rectangle>
        <Rectangle x:Name="F" Grid.Row="0"  RadiusX="20" RadiusY="20" Grid.Column="3" Fill="Ivory" Height="Auto" Margin="0 0 0 0" StrokeThickness="2" Stroke="Black" MouseLeftButtonDown="F_MouseLeftButtonDown" MouseLeftButtonUp="F_MouseLeftButtonUp" MouseLeave="F_MouseLeave" MouseEnter="F_MouseEnter"></Rectangle>
        <Rectangle x:Name="FSharp1" Grid.Row="0" Grid.Column="3" Fill="Black" Height="300" VerticalAlignment="Top"  Width="40" HorizontalAlignment="Right" Margin="0 0 0 0 " StrokeThickness="1" Stroke="Black" MouseLeftButtonDown="FSharp1_MouseLeftButtonDown" MouseLeftButtonUp="FSharp1_MouseLeftButtonUp" MouseEnter="FSharp1_MouseEnter" MouseLeave="FSharp1_MouseLeave"></Rectangle>
        <Rectangle x:Name="G" Grid.Row="0"  RadiusX="20" RadiusY="20" Grid.Column="4" Fill="Ivory" Height="Auto" Margin="0 0 0 0" StrokeThickness="2" Stroke="Black" MouseLeftButtonDown="G_MouseLeftButtonDown" MouseLeftButtonUp="G_MouseLeftButtonUp" MouseLeave="G_MouseLeave" MouseEnter="G_MouseEnter"></Rectangle>
