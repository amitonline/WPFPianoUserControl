        <Rectangle x:Name="B" Grid.Row="0"  RadiusX="20" RadiusY="20"  Grid.Column="6" Fill="Ivory" Height="Auto" Margin="0 0 0 0" StrokeThickness="2" Stroke="Black" MouseLeftButtonDown="B_MouseLeftButtonDown" MouseLeftButtonUp="B_MouseLeftButtonUp" MouseLeave="B_MouseLeave" MouseEnter="B_MouseEnter"></Rectangle>
        <Rectangle x:Name="ASharp2" Grid.Row="0" Grid.Column="6" Fill="Black" Height="300" VerticalAlignment="Top"  Width="40" HorizontalAlignment="Left" Margin="0 0 0 0 " StrokeThickness="1" Stroke="Black" MouseLeftButtonDown="ASharp2_MouseLeftButtonDown" MouseLeftButtonUp="ASharp2_MouseLeftButtonUp" MouseEnter="ASharp2_MouseEnter" MouseLeave="ASharp2_MouseLeave"></Rectangle>
